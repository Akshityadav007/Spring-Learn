package com.myProject3.BeanProperty;

public class Laptop {
	private String brand;
	
	
	
	public Laptop(String brand) {						// 2)
		super();										// 2)
		this.brand = brand;								// 2)
	}													// 2)

	public String getBrand() {
		return brand;
	}

	public void setBrand(String brand) {
		this.brand = brand;
	}

	@Override
	public String toString() {
		return "Laptop [brand=" + brand + "]";
	}
}
